<!-- db에 저장된 api값 조회 sql -->
<mapper namespace="com.midproject.tripin.repository.PlaceViewRepo">

<!-- db저장값 조회 -->
<select id="getAllPlaces" resultType="com.midproject.tripin.model.PlaceVO">
	SELECT * 
	FROM (
		SELECT * FROM dests
		ORDER BY dbms_random.value )
</select>


</mapper>