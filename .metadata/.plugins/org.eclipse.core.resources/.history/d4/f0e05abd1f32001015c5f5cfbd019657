package com.midproject.tripin.util;

public class PlaceThemeSort {
	public static String mapTheme(String cat1, String cat2, String cat3) {
		if("A01".equals(cat1)) return "자연"; //자연
		if ("A02".equals(cat1) && "A0201".equals(cat2)) return "문화"; //문화
        if ("A02".equals(cat1) && "A0202".equals(cat2)) return "레저"; //레저
        if ("A05".equals(cat1)) return "맛집"; //맛집
        if ("B02".equals(cat1)) return "숙소"; //숙소
        if ("A04".equals(cat1)) return "쇼핑"; //쇼핑
        if ("A02".equals(cat1) && "A0207".equals(cat2)) return "축제"; //축제
		
		return "기타"; //기타
	}
}
